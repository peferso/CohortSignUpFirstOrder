@article{TAYLOR1964580,
title = "Unitarity, hermiticity and discontinuity relations",
journal = "Nuclear Physics",
volume = "58",
pages = "580 - 592",
year = "1964",
issn = "0029-5582",
doi = "https://doi.org/10.1016/0029-5582(64)90568-1",
url = "http://www.sciencedirect.com/science/article/pii/0029558264905681",
author = "John R. Taylor",
abstract = "The relationship between unitarity, Hermitian-analyticity and the discontinuity relations for the T matrix is discussed. The generalized discontinuity relations, sometimes called generalized unitarity, are examined and their theoretical status discussed. It is shown that symmetry of the T matrix has no relevance to these properties. Finally all these properties are proved in the framework of the multichannel Schrödinger theory. It is shown that in Schrödinger theory the discontinuity relations arise naturally from the definition of the S matrix in terms of ingoing and outgoing waves and that unitarity and Hermitian analyticity hold when the potential matrix is Hermitian."
}

